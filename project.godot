; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Wardens Asylum"
config/description="Playable with DualShoch4 GamePad.
Contorls on Gamepad:
D-Pad to Move
X(Cross) to Interact and Use Right Hand
Square to use Left Hand
Triangle to use Inventory (Still needs a mouse to control inventory atm)
Circle will be for Dashing in future updates


Controls on Computer:
Arrow keys to move.
R to use RightHhand
F to use Left Hand
Tab or I to use the Inventory System"
config/version="0.1"
config/tags=PackedStringArray("game", "project", "tutorial")
run/main_scene="res://UI/Main Menu/main_menu.tscn"
config/features=PackedStringArray("4.3", "Mobile")
run/max_fps=60
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

TransitionChangeManager="*res://singletons/transition_manager.tscn"
FreezeEngineManager="*res://singletons/freeze_engine.gd"
Global="*res://singletons/Global.gd"
InventoryStateManager="*res://singletons/player_inventory_tracker.gd"
AsylumModeManager="*res://scenes/levels/Asylum Royale/asylum_mode.gd"
WorldNavigationManager="*res://singletons/WorldNavigation.gd"

[display]

window/size/mode=3
window/size/initial_position_type=2
window/size/resizable=false
window/size/always_on_top=true
window/size/extend_to_title=true
window/stretch/mode="viewport"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[file_customization]

folder_colors={
"res://Actors/": "red",
"res://Objects/": "teal",
"res://UI/": "pink",
"res://Utility/": "purple",
"res://assets/": "blue",
"res://scenes/": "orange",
"res://scripts/": "yellow",
"res://singletons/": "pink"
}

[global_group]

Danger="AI avoidance factor"
Enemy="AI Agent"
Player="Player detection class"

[gui]

theme/custom="res://themes/text_theme.tres"

